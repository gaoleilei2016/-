
.list-view {
	margin-top: 0rpx;
	margin-bottom: 20rpx;
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding-bottom: env(safe-area-inset-bottom);
}
/*tabbar start*/
::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}
.tab-view::before {
	content: '';
	position: absolute;
	border-bottom: 1rpx solid #eaeef1;
	-webkit-transform: scaleY(0.5);
	-ms-transform: scaleY(0.5);
	    transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 0;
}
.tab-view {
	width: 100%;
	height: 100rpx;
	overflow: hidden;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	position: relative;
	top: 0;
	
	left: 0;
	z-index: 99;
	background: #fff;
	white-space: nowrap;
}
.tab-bar-item {
	padding: 0;
	height: 100rpx;
	min-width: 320rpx;
	line-height: 100rpx;
	margin: 0 28rpx;
	display: inline-block;
	text-align: center;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.tab-bar-title {
	height: 100rpx;
	line-height: 100rpx;
	font-size: 32rpx;
	color: #999;
	font-weight: 400;
}
.active {
	border-bottom: 6rpx solid #5677fc;
}
.active .tab-bar-title {
	color: #5677fc !important;
	font-size: 32rpx;
	font-weight: bold;
}
/*tabbar end*/
