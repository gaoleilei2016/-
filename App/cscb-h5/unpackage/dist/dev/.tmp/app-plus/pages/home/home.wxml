<view><cu-custom vue-id="1" bgColor="bg-white" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content"><view class="cu-bar search"><view class="action"><text>{{city}}</text></view><view class="search-form round"><text class="cuIcon-search"></text><input disabled="false" adjust-position="{{false}}" type="text" placeholder="输入搜索关键词" confirm-type="search" data-event-opts="{{[['tap',[['cutTab',['$event']]]]]}}" bindtap="__e"/></view></view></view></cu-custom><swiper class="card-swiper square-dot" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500" indicator-color="#8799a3" indicator-active-color="#0081ff" data-event-opts="{{[['change',[['cardSwiper',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="{{[cardCur==index?'cur':'']}}"><view class="swiper-item"><image lazy-load="true" src="{{item.image}}" mode="aspectFill"></image></view></swiper-item></block></swiper><view class="cu-bar bg-white"><view class="action"><text class="text-black text-bold">挂单推荐</text></view><view data-event-opts="{{[['tap',[['more',['$event']]]]]}}" class="action" bindtap="__e"><text>更多<text class="cuIcon-right"></text></text></view></view><list-item class="vue-ref" vue-id="2" showkm="{{false}}" mylist="{{getTaskList}}" data-ref="listItem" data-event-opts="{{[['^navigateTo',[['navigate']]]]}}" bind:navigateTo="__e" bind:__l="__l"></list-item><load-tip vue-id="3" visible="{{isLoad}}" bind:__l="__l"></load-tip></view>