::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}
.tab-view::before {
	content: '';
	position: absolute;
	border-bottom: 1rpx solid #eaeef1;
	-webkit-transform: scaleY(0.5);
	-ms-transform: scaleY(0.5);
	    transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 0;
}
.tab-view {
	width: 100%;
	height: 100rpx;
	overflow: hidden;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	position: relative;
	top: 0;
	
	left: 0;
	z-index: 99;
	background: #fff;
	white-space: nowrap;
}
.tab-bar-item {
	padding: 0;
	height: 100rpx;
	min-width: 197rpx;
	line-height: 100rpx;
	margin: 0 28rpx;
	display: inline-block;
	text-align: center;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.tab-bar-title {
	height: 100rpx;
	line-height: 100rpx;
	font-size: 32rpx;
	color: #999;
	font-weight: 400;
}
.active {
	border-bottom: 6rpx solid #5677fc;
}
.active .tab-bar-title {
	color: #5677fc !important;
	font-size: 32rpx;
	font-weight: bold;
}
/*tabbar end*/
.scoll-y {
	height: 100%;
}
.line-height {
	line-height: 50rpx;
}
.time-verticals {
	z-index: 1000;
	margin-left: 70rpx;
	list-style-type: none;
	border-left: 1px solid transparent;
	padding: 0px;
	height: auto;
}
.time-verticals ._li {
	height: 32px;
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
}
.time-verticals ._li ._a {
	display: inline-block;
	margin-left: 20px;
	text-decoration: none;
	-webkit-align-self: center;
	    -ms-flex-item-align: center;
	        align-self: center;
	color: #000;
	width: 85%;
}
.time-verticals ._li ._b:before {
	content: '';
	position: absolute;
	left: -6px;
	width: 1px;
	height: 7px;
	border-top: 13px transparent dashed;
	border-left: 5px transparent dashed;
	border-right: 5px transparent dashed;
	border-bottom: 12px #707070 solid;
}
.time-verticals ._li ._span {
	position: absolute;
	color: #fff;
	left: -6px;
}
