age src="{{imgList[index]}}" mode="aspectFill"></image></block><block wx:if="{{item.g3!=-1}}"><view style="width:100%;height:100%;">{{item.$orig}}</view></block><block wx:if="{{item.g4!=-1}}"><view style="width:100%;height:100%;">{{item.$orig}}</view></block><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<1}}"><view data-event-opts="{{[['tap',[['clk']]]]}}" class="bg-gray" bindtap="__e"><text class="cuIcon-add"></text></view></block></view></view></view></view><view class="flex justify-between margin-tb padding bg-white"><view style="width:15%;"><image class="round bg-black" style="width:90rpx;height:90rpx;" src="{{form.seller_brand_logo}}" mode="aspectFit"></image></view><view data-event-opts="{{[['tap',[['navigationTo',['$event']]]]]}}" class="flex flex-direction justify-around" style="width:65%;" bindtap="__e"><text class="text-bold ">{{form.seller_title}}</text><text class="text-sm">联系电话:<text>{{form.link_mobile}}</text></text><text class="text-sm">地址:<text>{{form.seller_address}}</text><text class="text-theme">(导航到店)</text></text></view><view class="flex flex-direction justify-center" style="width:20%;height:150rpx;"><image style="width:130rpx;height:130rpx;" src="http://txcos.kelinteng.com/uploads/20191014/qrcode.png" mode="aspectFit"></image></view></view><view class="cu-tabbar-height"></view><view class="cu-tabbar-height"></view><block wx:if="{{form.obj_type==257}}"><view data-event-opts="{{[['tap',[['submitForm',['$event']]]]]}}" class="cu-bar foot block margin-bottom" bindtap="__e"><button class="cu-btn lg bg-theme block" style="width:90%;margin:0 auto;">确认提交</button></view></block></view>